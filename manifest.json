{
	"manifest_version": 2,
	"name": "FoxRedux",
	"version": "0.1",
	"devtools_page": "devtools.html",
	"content_scripts": [
		{
			"js": ["content.js"],
			"matches": ["<all_urls>"]
		}
	],
	"permissions": ["tabs","activeTab"],
	"browser_action": {
		"default_icon": "PISF.png",
		"default_popup": "popup.html",
		"default_title": "FoxRedux app "
	},
	"web_accessible_resources": [
		"*"
	]
}
